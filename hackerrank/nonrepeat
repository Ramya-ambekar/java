
public class nonrepeat {
	public static void main(String[] args) {
		String s="ramyarr";
		int n=s.length();
		//System.out.print(n);
		for(int i=0;i<n;i++){
			boolean a=true;
			for(int j=0;j<n;j++)
			{
				if(i!=j && s.charAt(i)==s.charAt(j)){
				 a=false;	 
				break;	
				}			
			}
					
		if(a==true){
			System.out.print(s.charAt(i));
			break;
		}
		}
	}

}
